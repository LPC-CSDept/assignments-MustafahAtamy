#include<iostream>
using namespace std;

  int main(){
  int size1,size2;
  cout<<"Enter size of array 1 and array 2" 
// array 1 must be grater than array 2 for statement to hold true
  cin>>size1, size2;
  if(size1>size2){
   cout<<"False"<<endl;
   return 0;
   }
// The code above is to legitimize the statement written above it   
 
int number1[size1],number2[size2];
for(int i=0,i<size1;1++){
cin>>number1[i]; 
}
// Here I am using a single loop
for(int i =0;i<size2;i++) {
cin>>number1[i];
}
int k=0
for(int i=0;i<size2;i++) {
if(number1[k]==number2[i]){ k++;
}
if (k==size1){
break;
}
// if k==size 1, then teh array1 is a subset of array2.
if(k==size1){
cout<<"Array1 is a subset of Array2"<<endl;
}
else{
cout<<"Array1 is not a subset of Array2"<<endl;
}
}
//I made an if loop to add a cout for any different outcomes.
